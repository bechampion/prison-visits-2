<tr>
  <td><%= visitor.first_name %> <%= visitor.last_name %></td>
  <td><%= visitor.date_of_birth.to_s(:short_nomis) %></td>
  <td>
    <% if lead %>
      <%= mail_to @visit.contact_email_address %>
    <% end %>
  </td>
  <td>
    <% if lead %>
      <%= @visit.contact_phone_no %>
    <% end %>
  </td>
  <td>
    <% if visitor.banned %>
      <span class="tag tag--error">Banned</span>
    <% end %>
    <% if visitor.not_on_list %>
      <span class="tag tag--warning">Not listed</span>
    <% end %>
  </td>
</tr>